<!DOCTYPE html>
<html>
  <head>
    <title><%= pkg.name %></title>
    <script src="http://code.jquery.com/jquery.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.0.6/angular.min.js"></script>
    <link rel="stylesheet" type="text/css" href="<%= css %>" media="all" />
  </head>
  <body>
    <button id="authorize-button" style="visibility: hidden">Authorize</button>
    <script type="text/javascript" src="<%= js %>"></script>
    <script src="https://apis.google.com/js/client.js?onload=handleClientLoad"></script>
    <div ng-controller="ListCtrl">
      <form ng-submit="updatePageVisits()">
        <div class="row">
          <div class="large-2 columns">
            <label>Start Date</label>
            <input type="text" ng-model="filter.startDate" datepicker/>
          </div>
          <div class="large-2 columns">
            <label>End Date</label>
            <input type="text" ng-model="filter.endDate" datepicker/>
          </div>
          <div class="large-8 columns">
            <label>&nbsp;</label>
            <input type="submit" value="Update" class="button small"/>
          </div>
        </div>
      </form>
      <div class="row">
        <table>
          <thead>
            <tr><th>Page</th><th>Visits</th></tr>
          </thead>
          <tbody>
            <tr ng-repeat="pageVisit in pageVisits">
              <td>{{pageVisit.page}}</td><td>{{pageVisit.visits}}</td>
            </tr>
          </tbody>
        </table>
      </div>
    </div>
  </body>
</html>
